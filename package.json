{
  "name": "energy-audit-panel",
  "version": "1.0.0",
  "description": "Energy Audit Panel - Innovative Web-Based Energy Audit Platform",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server/server.js",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "node server/src/database/runSeeders.js",
    "setup-db": "npm run migrate && npm run seed",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "setup": "node setup.js",
    "health-check": "node server/src/scripts/verify-backend-health.js",
    "setup:db": "node server/src/scripts/setup-database.js",
    "verify:db": "node server/src/scripts/verify-database.js",
    "migrate:down": "node server/src/scripts/run_migrations.js --down",
    "seed:tags": "cd server && npx ts-node src/scripts/seed-tags.ts"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/file-saver": "^2.0.7",
    "@types/helmet": "^0.0.48",
    "@types/lodash.debounce": "^4.0.9",
    "@types/morgan": "^1.9.9",
    "@types/papaparse": "^5.3.16",
    "@types/react-window": "^1.8.8",
    "@types/xlsx": "^0.0.35",
    "concurrently": "^8.0.1"
  },
  "dependencies": {
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@mui/styles": "^5.17.1",
    "@mui/system": "^5.17.1",
    "@mui/utils": "^5.17.1",
    "@mui/x-data-grid": "^5.17.24",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/form-data": "^2.2.1",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "chartjs-plugin-zoom": "^2.2.0",
    "compression": "^1.8.0",
    "docx": "^9.5.0",
    "file-saver": "^2.0.5",
    "form-data": "^4.0.2",
    "helmet": "^8.1.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "knex": "^3.1.0",
    "mammoth": "^1.9.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.1",
    "papaparse": "^5.5.2",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "react-window": "^1.8.11",
    "recharts": "^2.15.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "three": "^0.176.0",
    "winston": "^3.17.0",
    "ws": "^8.18.2",
    "xlsx": "^0.18.5",
    "zod": "^3.24.4"
  }
}
